pipeline{
	agent any
	environment {
    	MAJOR_VERSION = 1
  	}

	stages {
		stage('Clean Up') {
			steps {
				echo "Code Clean up In Progress"	
			}
		}
		
		stage('Code Compile') {
			steps {
				echo "Code Clean Up In Progress"	
			}
		}
		
		stage('Code Build') {
			steps {
				echo "Application Build In Progress"	
			}
		}
		
		stage('Deploy') {
			steps {
				echo "Application Deployment In Progress"	
			}
		}
	}
}